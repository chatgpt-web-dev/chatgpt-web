{
  "name": "chatgpt-web",
  "type": "module",
  "version": "2.13.3",
  "private": false,
  "description": "ChatGPT Web",
  "author": "ChatGPT Web Contributors <dev@chatgpt-web.dev>",
  "keywords": [
    "chatgpt-web",
    "chatgpt",
    "chatbot",
    "vue"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
